{
  "name": "dogecloud-actions",
  "version": "1.0.0",
  "description": "GitHub Actions for Dogecloud deployment",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:unit": "jest --testPathIgnorePatterns=integration",
    "test:integration": "jest --testPathPatterns=integration",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "package": "ncc build --source-map --license licenses.txt"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/dogecloud-actions.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup",
    "dogecloud",
    "deployment"
  ],
  "author": "Your Name",
  "license": "MIT",
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@actions/github": "^6.0.1",
    "@aws-sdk/client-s3": "^3.876.0",
    "axios": "^1.11.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "@vercel/ncc": "^0.38.3",
    "dotenv": "^17.2.1",
    "jest": "^30.1.1",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.1",
    "typescript": "^5.9.2"
  }
}
