{
  "name": "dogecloud-actions",
  "version": "1.0.0",
  "description": "GitHub Actions for deploying to DogeCloud with concurrent uploads",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "package": "ncc build src/main.ts -o dist --source-map --license licenses.txt",
    "all": "npm run build && npm run package",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "github-actions",
    "dogecloud",
    "deploy",
    "cdn"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^6.0.0",
    "@aws-sdk/client-s3": "^3.456.0",
    "axios": "^1.11.0",
    "mime-types": "^2.1.35"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^20.8.10",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vercel/ncc": "^0.38.3",
    "dotenv": "^17.2.1",
    "eslint": "^8.53.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=20"
  }
}
